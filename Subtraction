
#include <iostream>
using namespace std;

int main() {
    int num1, num2, sum;

    // Input two numbers
    cout << "Enter the first number: ";
    cin >> num1;
    cout << "Enter the second number: ";
    cin >> num2;

    // Calculate sub
    sub = num1 + num2;

    // Display the sub
    cout << "The sub of " << num1 << " and " << num2 << " is " << sum << endl;

    return 0;
}
